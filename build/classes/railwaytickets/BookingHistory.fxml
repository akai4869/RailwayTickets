<?xml version="1.0" encoding="UTF-8"?>

<?import com.jfoenix.controls.JFXButton?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane id="AnchorPane" prefHeight="520.0" prefWidth="800.0" xmlns="http://javafx.com/javafx/8.0.141" xmlns:fx="http://javafx.com/fxml/1" fx:controller="railwaytickets.BookingHistoryController">
   <children>
      <AnchorPane prefHeight="41.0" prefWidth="800.0" style="-fx-background-color: #000000;">
         <children>
            <JFXButton layoutX="715.0" layoutY="6.0" onAction="#onSignOut" style="-fx-background-color: #FFFFFF;" text="Sign Out">
               <font>
                  <Font name="System Bold" size="14.0" />
               </font>
            </JFXButton>
         </children>
      </AnchorPane>
      <Label layoutX="285.0" layoutY="69.0" text="Booked Ticket History">
         <font>
            <Font size="24.0" />
         </font>
      </Label>
      <TableView fx:id="bookedTicketTable" layoutX="180.0" layoutY="122.0" prefHeight="239.0" prefWidth="442.0">
        <columns>
          <TableColumn fx:id="pnrNo" prefWidth="186.0" text="Pnr No" />
          <TableColumn fx:id="trainNo" prefWidth="73.0" text="Train No" />
            <TableColumn fx:id="dateofJourney" prefWidth="115.0" text="Date of Journey" />
            <TableColumn fx:id="noOfSeats" prefWidth="67.0" text="Seats" />
        </columns>
      </TableView>
      <JFXButton fx:id="showDetails" layoutX="311.0" layoutY="390.0" onAction="#showDetails" prefHeight="41.0" prefWidth="180.0" style="-fx-background-color: #000000;" text="Show Details" textFill="WHITE">
         <font>
            <Font name="System Bold" size="14.0" />
         </font>
      </JFXButton>
      <JFXButton layoutX="305.0" layoutY="443.0" onAction="#bookAnotherTicket" prefHeight="41.0" prefWidth="191.0" style="-fx-background-color: #000000;" text="Book Another Ticket" textFill="WHITE">
         <font>
            <Font name="System Bold" size="14.0" />
         </font>
      </JFXButton>
   </children>
</AnchorPane>
